{% extends "index.twig" %}

{% block content %}

    <h2>{{ "Icons"|_ }}</h2>

    <div class="row">
    {% for icon in icons %}
        <div class="col-xs-1 well well-small">
            <i style="font-size: 16px;" class="glyphicon {{ icon }}"></i>
            {{ icon }}
        </div>
    {% endfor %}
    </div>

{% endblock %}
